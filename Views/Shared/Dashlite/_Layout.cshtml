<body class="nk-body bg-lighter npc-general has-sidebar">
    <div class="nk-app-root">
        <div class="nk-main ">
            <div class="nk-sidebar nk-sidebar-fixed is-theme" data-content="sidebarMenu">
                <div class="nk-sidebar-element nk-sidebar-head">
                    <div class="nk-menu-trigger">
                        <a href="#" class="nk-nav-toggle nk-quick-nav-icon d-xl-none" data-target="sidebarMenu"><em class="icon ni ni-arrow-left"></em></a>
                        <a href="#" class="nk-nav-compact nk-quick-nav-icon d-none d-xl-inline-flex" data-target="sidebarMenu"><em class="icon ni ni-menu"></em></a>
                    </div>
                    <div class="nk-sidebar-brand">
                        <a href="~/Theme/DASHLITE/demo1/src/html/index.html" class="logo-link nk-sidebar-logo">
                            <h6 style="color:white">SBM SYSTEM MONITOR</h6>
                        </a>
                    </div>
                </div>
                <div class="nk-sidebar-element nk-sidebar-body">
                    <div class="nk-sidebar-content">
                        <div class="nk-sidebar-menu" data-simplebar>
                            <ul class="nk-menu">
                                <li class="nk-menu-heading">
                                    <h6 class="overline-title text-primary-alt">Server</h6>
                                </li>
                                <li class="nk-menu-item">
                                    <a href="@Url.Action("QA", "Report")" class="nk-menu-link">
                                        <span class="nk-menu-icon"><em class="icon ni ni-line-chart"></em></span>
                                        <span class="nk-menu-text">DEVELOPMENT<br />(172.18.100.172)</span>
                                    </a>
                                </li>
                                <li class="nk-menu-item">
                                    <a href="@Url.Action("PRD", "Report")" class="nk-menu-link">
                                        <span class="nk-menu-icon"><em class="icon ni ni-line-chart"></em></span>
                                        <span class="nk-menu-text">PRODUCTION<br />(172.18.100.84)</span>
                                    </a>
                                </li>
                                <li class="nk-menu-item">
                                    <a href="@Url.Action("PRODUCTION_PUB", "Report")" class="nk-menu-link">
                                        <span class="nk-menu-icon"><em class="icon ni ni-line-chart"></em></span>
                                        <span class="nk-menu-text">PRODUCTION (PUBLISH)<br />(0.0.0.0)</span>
                                        <span class="nk-menu-badge">OFFLINE</span>
                                    </a>
                                </li>

                                @if (ROLE == "SYSTEM-ADMINA" || ROLE == "SUPER-ADMINA" || ROLE == "SYSTEM-MANAGERA")
                                {
                                    <li class="nk-menu-heading">
                                        <h6 class="overline-title text-primary-alt">Master Data</h6>
                                    </li>

                                    <li class="nk-menu-item">
                                        <a href="@Url.Action("Category", "MasterData")" class="nk-menu-link">
                                            <span class="nk-menu-icon"><em class="icon ni ni-bookmark"></em></span>
                                            <span class="nk-menu-text">Category</span>
                                        </a>
                                    </li>

                                    <li class="nk-menu-item">
                                        <a href="@Url.Action("Department", "MasterData")" class="nk-menu-link">
                                            <span class="nk-menu-icon"><em class="icon ni ni-user-group"></em></span>
                                            <span class="nk-menu-text">Department</span>
                                        </a>
                                    </li>

                                    <li class="nk-menu-item">
                                        <a href="@Url.Action("Group", "MasterData")" class="nk-menu-link">
                                            <span class="nk-menu-icon"><em class="icon ni ni-link-group"></em></span>
                                            <span class="nk-menu-text">Group</span>
                                        </a>
                                    </li>

                                    <li class="nk-menu-item">
                                        <a href="@Url.Action("UserDC", "MasterData")" class="nk-menu-link">
                                            <span class="nk-menu-icon"><em class="icon ni ni-user"></em></span>
                                            <span class="nk-menu-text">User</span>
                                        </a>
                                    </li>

                                    <li class="nk-menu-item">
                                        <a href="@Url.Action("Report", "MasterData")" class="nk-menu-link">
                                            <span class="nk-menu-icon"><em class="icon ni ni-bar-chart"></em></span>
                                            <span class="nk-menu-text">Report</span>
                                        </a>
                                    </li>
                                
                                    <li class="nk-menu-heading">
                                        <h6 class="overline-title text-primary-alt">Setting</h6>
                                    </li>

                                    <li class="nk-menu-item">
                                        <a href="@Url.Action("Index", "Profile")" class="nk-menu-link">
                                            <span class="nk-menu-icon"><em class="icon ni ni-user"></em></span>
                                            <span class="nk-menu-text">User Setting</span>
                                        </a>
                                    </li>
                                }
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <div class="nk-wrap ">
                <div class="nk-header nk-header-fixed is-theme">
                    <div class="container-fluid">
                        <div class="nk-header-wrap">
                            <div class="nk-menu-trigger d-xl-none ms-n1">
                                <a href="#" class="nk-nav-toggle nk-quick-nav-icon" data-target="sidebarMenu"><em class="icon ni ni-menu"></em></a>
                            </div>
                            <div class="nk-header-brand d-xl-none">
                                <a href="html/index.html" class="logo-link">
                                    <h6 style="color:white">SBM SYSTEM MONITOR</h6>
                                </a>
                            </div>
                            <div class="nk-header-news d-none d-xl-block">

                            </div>
                            <div class="nk-header-tools">
                                <ul class="nk-quick-nav">

                                    <li class="dropdown user-dropdown">
                                        <a href="#" class="dropdown-toggle" data-bs-toggle="dropdown">
                                            <div class="user-toggle">
                                                <div class="user-avatar sm">
                                                    <em class="icon ni ni-user-alt"></em>
                                                </div>
                                                <div class="user-info d-none d-md-block">
                                                    <div class="user-status">@ViewBag.GROUPS_PBI</div>
                                                    <div class="user-name dropdown-indicator">@ViewBag.NAME</div>
                                                </div>
                                            </div>
                                        </a>
                                        <div class="dropdown-menu dropdown-menu-md dropdown-menu-end dropdown-menu-s1">
                                            <div class="dropdown-inner user-card-wrap bg-lighter d-none d-md-block">
                                                <div class="user-card">
                                                    <div class="user-avatar">
                                                        <span>@ViewBag.INITIALS</span>
                                                    </div>
                                                    <div class="user-info">
                                                        <span class="lead-text">@ViewBag.NAME</span>
                                                        <span class="sub-text">@ViewBag.GROUPS_PBI (@ViewBag.DEPARTMENT)</span>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="dropdown-inner">
                                                <ul class="link-list">
                                                    <li><a class="dark-switch" href="#"><em class="icon ni ni-moon"></em><span>Dark Mode</span></a></li>
                                                </ul>
                                            </div>
                                            <div class="dropdown-inner">
                                                <ul class="link-list">
                                                    <li><a href="@Url.Action("Logout", "Authentication")"><em class="icon ni ni-signout"></em><span>Sign out</span></a></li>
                                                </ul>
                                            </div>
                                        </div>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="nk-content ">
                    <div class="container-fluid">
                        <div class="nk-content-inner">
                            <div class="nk-content-body">
                                @RenderBody()
                            </div>
                        </div>
                    </div>
                </div>

                <div class="nk-footer">
                    <div class="container-fluid">
                        <div class="nk-footer-wrap">
                            <div class="nk-footer-copyright">
                                Copyright &copy; 1991 - @DateTime.Now.ToString("yyyy") <b>PT SHIMANO BATAM</b>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <script src="~/Theme/DASHLITE/demo1/src/assets/js/bundle.js"></script>
    <script src="~/Theme/DASHLITE/demo1/src/assets/js/scripts.js"></script>
