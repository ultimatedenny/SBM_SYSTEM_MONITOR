    <script src="~/Theme/DASHLITE/demo1/src/assets/js/charts/gd-analytics.js"></script>
    <script src="~/Theme/DASHLITE/demo1/src/assets/js/charts/gd-invest.js"></script>
    <script src="~/Theme/DASHLITE/demo1/src/assets/js/example-sweetalert.js"></script>
    <script src="~/Theme/DASHLITE/demo1/src/assets/js/example-toastr.js"></script>
    <script src="~/Theme/DASHLITE/demo1/src/assets/js/libs/dual-listbox.js"></script>
    <script src="~/Theme/DASHLITE/demo1/src/assets/js/libs/jstree.js"></script>
    <script src="~/Theme/DASHLITE/demo1/src/assets/js/example-tree.js"></script>
    <script src="~/Theme/DASHLITE/demo1/src/assets/js/libs/editors/quill.js"></script>
    <script src="~/Theme/DASHLITE/demo1/src/assets/js/libs/editors/summernote.js"></script>
    <script src="~/Theme/DASHLITE/demo1/src/assets/js/libs/editors/tinymce.js"></script>
    <script src="~/Theme/DASHLITE/demo1/src/assets/js/editors.js"></script>
    <script src="~/Theme/DASHLITE/demo1/src/assets/js/libs/datatable-btns.js"></script>
    <script src="~/Theme/DASHLITE/demo1/src/assets/js/libs/fullcalendar.js"></script>
    <script src="~/Theme/DASHLITE/demo1/src/assets/js/apps/calendar.js"></script>
    <script src="~/Theme/DASHLITE/demo1/src/assets/js/libs/jkanban.js"></script>
    <script src="~/Theme/DASHLITE/demo1/src/assets/js/apps/kanban.js"></script>
    <script src="~/Theme/DASHLITE/demo1/src/assets/js/libs/jqvmap.js"></script>
    <script src="~/Theme/DASHLITE/demo1/src/assets/js/libs/tagify.js"></script>
    <script src="https://unpkg.com/isotope-layout@3/dist/isotope.pkgd.js"></script>
    <script src="https://malsup.github.io/jquery.blockUI.js"></script>
    <script src="~/Theme/bootstrap-multiselect.min.js"></script>


    <script>

        $(document).ajaxStop($.unblockUI);

        $(document).ajaxComplete(function () {
            $.unblockUI()
        });

        $(document).ajaxStart(function () {
            $.blockUI({
                css: {
                    width: '350px',
                    top: '10px',
                    left: '',
                    right: '10px',
                    border: 'none',
                    padding: '5px',
                    backgroundColor: '#F2F2F2',
                    '-webkit-border-radius': '10px',
                    '-moz-border-radius': '10px',
                    opacity: .6,
                    color: '#060606'
                }
            });
        }); 

        function CALL_NOTIF(Msg) {
            alert(Msg)
        }
        
    </script>
</body>

</html>